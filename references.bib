@article{becker_repositorien_2014,
	title = {Repositorien und das Semantic Web},
	url = {https://depositonce.tu-berlin.de/handle/11303/5330},
	abstract = {Repositorien sind Systeme zur sicheren Speicherung und Publikation digitaler Objekte und der die Objekte beschreibenden Metadaten. Repositorien bieten ihre Inhalte hauptsächlich über Weboberflächen an, die auf die menschliche Wahrnehmung ausgerichtet sind. Die gespeicherten Daten stellen sie nicht so bereit, dass sie einfach durch Computer verarbeitet werden können oder sie verstecken sie hinter spezifischen Schnittstellen. Dabei eignen sich die in Repositorien gehaltenen Daten besonders gut dazu, als Linked Data in das Semantic Web eingebunden zu werden, da Metadaten bereits vorhanden sind und nicht erst generiert oder manuell erfasst werden müssen. Wie Inhalte von Repositorien in die Linked (Open) Data Cloud eingebunden werden können und welche Besonderheiten von Repositorien dabei beachtet werden müssen, sind die zentralen Fragestellungen dieser Arbeit.},
	author = {Becker, Pascal-Nicolas},
	urldate = {2023-12-17},
	date = {2014-07-02},
	langid = {german},
}

